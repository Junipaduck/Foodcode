<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.itwillbs.foodcode.mapper.MenuMapper">

 <insert id="insertMenu">
	 	INSERT
	 		INTO menu
	 		VALUES (
	 			1
	 			,#{menu_name} 
	 			,#{menu_type} 
	 			,#{menu_price} 
	 			,#{menu_image} 
	 			,#{menu_content} 
	 			,1  
	 		)
	 </insert>

</mapper>
